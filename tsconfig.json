{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es2020",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "emitDeclarationOnly": true,
    "declaration": true,
    "outDir": "node_modules/.cache/tsbuild",
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "composite": true,
    "incremental": true,
    "outDir": "node_modules/.cache/tsbuild",
    "noEmit": false
  },
  "include": ["./**/*.ts", "./**/*.tsx"],
  "exclude": ["./node_modules"]
}
